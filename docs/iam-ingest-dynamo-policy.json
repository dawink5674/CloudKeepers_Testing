{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowPutItemOnEventsTable",
      "Effect": "Allow",
      "Action": [
        "dynamodb:PutItem"
      ],
      "Resource": "arn:aws:dynamodb:us-east-1:850104385746:table/honeylambda-events"
    },
    {
      "Sid": "AllowPublishToAlertsTopic",
      "Effect": "Allow",
      "Action": [
        "sns:Publish"
      ],
      "Resource": "arn:aws:sns:us-east-1:850104385746:honeylambda-alerts"
    }
  ]
}
